{
    "plugins": [],
    "source": {
        "include":["src"],
        "includePattern": ".(js|jsx)$",
        "excludePattern": "(node_modules|docs)"
    },
    "templates":{
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts":{
        "readme": "./README.md",
        "template": "node_modules/docdash",
        "recurse": true,
        "verbose": true,
        "destination":"./doc"
    }
}